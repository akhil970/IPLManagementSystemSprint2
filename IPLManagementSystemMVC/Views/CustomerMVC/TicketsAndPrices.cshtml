@model IEnumerable<IPLManagementSystemMVC.Models.CustomerTicketCategories>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Ticket_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Price)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Ticket_Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Price)
        </td>
    </tr>
}

</table>
